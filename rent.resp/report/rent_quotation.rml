        </td>
        <td>
          <para style="P5">[[o.date_created or '']]</para>
        </td>
      </tr>
      <tr>
        <td>
          <para style="P6">REFERENCIA:</para>
        </td>
        <td>
          <para style="P8">[[o.reference or '']]</para>
        </td>
        <td>
          <para style="P5">
            <font color="white"> </font>
          </para>
        </td>
        <td>
          <para style="P5">
            <font color="white"> </font>
          </para>
        </td>
      </tr>
