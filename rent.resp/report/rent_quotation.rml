      <tr>
        <td>
          <para style="P5">ITEM</para>
        </td>
        <td>
          <para style="P5">Equipo</para>
        </td>
        <td>
          <para style="P5">Modelo</para>
        </td>
        <td>
          <para style="P5">Marca</para>
        </td>
        <td>
          <para style="P5">Descripción</para>
        </td>
        <td>
          <para style="P5">Tipo de Tarifa</para>
        </td>
        <td>
          <para style="P5">$ Tarifa</para>
        </td>
      </tr>
    </blockTable>
    <blockTable colWidths="562.0" style="Table8">
      <tr>
        <td>
